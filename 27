Private Sub btnExportarAExcel_Click()

    Dim ruta As String
    
    ruta = CurrentProject.Path & "\Incidencias_Editar.xlsx"
    
    DoCmd.TransferSpreadsheet _
        TransferType:=acExport, _
        SpreadsheetType:=acSpreadsheetTypeExcel12Xml, _
        TableName:="qryExportarIncidencias", _
        FileName:=ruta, _
        HasFieldNames:=True
        
    MsgBox "Archivo exportado correctamente en: " & ruta, vbInformation

End Sub
