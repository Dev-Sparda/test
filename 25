def load_excel_file(filepath: str, origen: str):
    # ... (engine, usecols, skiprows) ...

    df = pd.read_excel(
        filepath,
        sheet_name='Movimientos',
        skiprows=skiprows,
        usecols=usecols,
        engine=engine
    )

    # Mapeo de columnas → incluir HAIRCUT y # títulos
    col_map = {
        'Fecha Liq.': 'FECHA_LIQ',
        'Branch': 'BRANCH',
        'Contraparte': 'CONTRAPARTE',
        'Envío / Recepción': 'ENVIO_RECEPCION',
        'Emisión': 'EMISION',
        '# títulos': 'TITULOS',      # <-- Aquí estaba el nombre real
        'Haircut': 'HAIRCUT',        # <-- Conservar Haircut
        'ISIN': 'ISIN'
    }
    df.rename(columns=col_map, inplace=True)

    # Eliminar columnas que no se usan (Monto, etc.)
    cols_to_drop = ['Precio Mercado', 'Precio c/Haricut', 'Monto $', 'Monto $ USD', 'Monto $ MXN']
    df.drop(columns=[c for c in cols_to_drop if c in df.columns], inplace=True)

    # Normalizar texto
    df['EMISION'] = df['EMISION'].apply(normalize_text)
    df['CONTRAPARTE'] = df['CONTRAPARTE'].apply(normalize_text)

    df['ID_ORIGEN'] = ORIGEN_ID[origen]

    return df
